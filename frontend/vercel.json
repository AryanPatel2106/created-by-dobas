{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://created-by-dobas.onrender.com/api/$1"
    },
    {
      "source": "/((?!assets/|favicon|manifest|sw\\.js|offline\\.html|\\.js$|\\.css$|\\.png$|\\.jpg$|\\.jpeg$|\\.gif$|\\.svg$|\\.ico$|\\.woff$|\\.woff2$|\\.ttf$|\\.eot$).*)",
      "destination": "/index.html"
    }
  ]
}